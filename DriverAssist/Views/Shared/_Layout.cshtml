<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <link rel="icon" type="image/png" href="~/img/favicon.ico">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Driver Assist App</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />

    <!-- Bootstrap core CSS     -->
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation library for notifications   -->
    <link href="~/Content/animate.min.css" rel="stylesheet" />
    <!--  Light Bootstrap Table core CSS    -->
    <link href="~/Content/light-bootstrap-dashboard.css" rel="stylesheet" />

    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300' rel='stylesheet' type='text/css'>
    <link href="~/Content/pe-icon-7-stroke.css" rel="stylesheet" />
    <link href="https://fonts.googleapis.com/css?family=Audiowide|Jura:400,600&subset=latin,latin-ext" rel='stylesheet' type="text/css">
    <link href="~/fonts/fontello.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="wrapper" data-image="~/img/sidebar-5.jpg">
        <div class="sidebar" data-color="green" data-image="../img/sidebar-5.jpg">
            <!--   you can change the color of the sidebar using: data-color="blue | azure | green | orange | red | purple" -->
           
            <div class="sidebar-wrapper">
                <div class="logo">
                    <a href="#" style="font-family: 'Jura', sans-serif; font-size: 28px; font-color: #FFFFFF;">
                        Driver Assist
                        <i class="pe-7s-car"></i>
                    </a>
                </div>
                <ul class="nav">
                    <li>
                        <a href="home.html">
                            <i class="pe-7s-home"></i>
                            <p>Home</p>
                        </a>
                    </li>
                    <li>
                        <a href="table.html">
                            <i class="pe-7s-note2"></i>
                            <p>Sensor tables</p>
                        </a>
                    </li>

                    <li>
                        <a href="speedometer.html">
                            <i class="pe-7s-car"></i>
                            <p>Speedometer</p>
                        </a>
                    </li>
                    <li>                        
                        <a href="@Url.Action("Maps", "Home")">
                            <i class="pe-7s-graph1"></i>
                            <p>Maps</p>

                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("About", "Home")">
                            <i class="pe-7s-graph1"></i>                           
                            <p>Stats</p>     
                        </a>
                    </li>
                    <li>
                        <a href="user.html">
                            <i class="pe-7s-user"></i>
                            <p>User Profile</p>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <i class="pe-7s-power"></i>
                            <p>Log out</p>
                        </a>
                    </li>
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <nav class="navbar navbar-default navbar-fixed">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <a class="navbar-brand" href="#">Maps</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                    <p>
                                        My account
                                        <b class="caret"></b>
                                    </p>
                                </a>
                                <ul class="dropdown-menu">
                                    <li><a href="#">Profile</a></li>
                                    <li><a href="user.html">Settings</a></li>
                                    <li class="divider"></li>
                                    <li><a href="#">Log out</a></li>
                                </ul>
                            </li>
                            <li>
                                <a href="#">
                                    <p>Log out</p>
                                </a>
                            </li>
                            <li class="separator hidden-lg hidden-md"></li>
                        </ul>
                    </div>
                </div>
            </nav>

            @RenderBody()
        </div>




    </div>

    

    <!--   Core JS Files   -->
    <script src="~/Scripts/jquery-1.10.2.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap.min.js" type="text/javascript"></script>
    <!--  Checkbox, Radio & Switch Plugins -->
    <script src="~/Scripts/bootstrap-checkbox-radio-switch.js"></script>
    <!--  Charts Plugin -->
    <script src="~/Scripts/chartist.min.js"></script>
    <!--  Notifications Plugin    -->
    <script src="~/Scripts/bootstrap-notify.js"></script>
    <!-- Light Bootstrap Table Core javascript and methods for Demo purpose -->
    <script src="~/Scripts/light-bootstrap-dashboard.js"></script>
    @RenderSection("scripts", required: false)

</body>

</html>
